<?xml version="1.0"?>
<launch>
    <arg name="camera_model"          default="zed" />
    <include file="$(find zed_wrapper)/launch/zed.launch">
        <arg name="camera_model"        value="$(arg camera_model)" />
        <arg name="stream"              value="" />
    </include>

    <!-- rviz -->
    <node pkg="rviz" type="rviz" name="rviz_ar" args="-d $(find ytlab_zed_modules)/rviz/user_zed.rviz" />
</launch>
